# Toollabs golang webserver image
# Name: docker-registry.tools.wmflabs.org/toollabs-golang-web
FROM {registry}/{image_prefix}-golang-base

RUN apt-get update \
    && DEBIAN_FRONTEND=noninteractive \
    apt-get install --yes \
    toollabs-webservice \
    && apt-get clean \
    && rm -rf /var/lib/apt/lists/*
